# GitHub App Configuration
GITHUB_APP_ID=your_github_app_id_here
GITHUB_PRIVATE_KEY=your_github_private_key_here
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# LLM Configuration
LLM_API_KEY=your_llm_api_key_here
LLM_PROVIDER=openai  # currently only openai is supported
LLM_MODEL=o3-mini  # currently only o3-mini is supported
LLM_TEMPERATURE=0
LLM_MAX_TOKENS=4000

# Optional Settings (with defaults)
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000
DEBUG=false

# SMEE Configuration (for development webhook forwarding)
SMEE_URL=your_smee_url_here
SMEE_TARGET=your_local_target_url_here
